.m stdlib & ;


.prog 0 start ;
.var int n 0 ;
.con in n ;
.list init int a n 0 ;
.var int i 0 ;
.for i 0 n 1 start ;
    .con in a[i] ;
.for end ;
.deinit i ;
.prog 0 end ;


.prog 1 start ;
.var int i 0 ;
.var int s 0 ;
.for i 0 n 1 start ;
    .arthm s = s + a[i] ;
.for end ;
.con out s ;
.prog 1 end ;


.run 0 1 ;
